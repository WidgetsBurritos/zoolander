<!-- Customer Story River -->
<div class="rsCustomer-story-header">
  <picture class="rsCustomer-story-headerBackground">
     <!--[if IE 9]><video style="display: none;"><![endif]-->
    <source class="rsCustomer-story-headerBackgroundContent" srcset="imgs/derek/banners/customer-story-background-3-1400.jpg 1x" media="all and (min-width: 1600px)" type="image/jpeg"/>
    <source class="rsCustomer-story-headerBackgroundContent" srcset="imgs/derek/banners/customer-story-background-3-1400.jpg 1x" media="all and (min-width: 1280px) and (max-width: 1599px)" type="image/jpeg"/>
    <source class="rsCustomer-story-headerBackgroundContent" srcset="imgs/derek/banners/customer-story-background-3-1400.jpg 1x" media="all and (min-width: 992px) and (max-width: 1279px)" type="image/jpeg"/>
    <source class="rsCustomer-story-headerBackgroundContent" srcset="imgs/derek/banners/customer-story-background-3-1400.jpg 1x" media="all and (min-width: 768px) and (max-width: 991px)" type="image/jpeg"/>
    <source class="rsCustomer-story-headerBackgroundContent" srcset="imgs/derek/banners/customer-story-background-3-1400.jpg 1x" media="all and (min-width: 480px) and (max-width: 767px)" type="image/jpeg"/>
    <source class="rsCustomer-story-headerBackgroundContent" srcset="imgs/derek/banners/customer-story-background-3-1400.jpg 1x" type="image/jpeg"/>
    <!--[if IE 9]></video><![endif]-->
    <img class="rsCustomer-story-headerBackgroundContent" src="imgs/derek/banners/customer-story-background-3-1400.jpg" alt="Test page banner">
  </picture>
  <div class="rsCustomer-story-headerContainer container">
    <div class="rsCustomer-story-headerInner">
      <!-- we only need cs-story-box if we want the scroll animation to run -->
      <div class="rsCustomer-story-text cs-story-box">
        <img class="rsCustomer-story-logo" src="imgs/derek/logos/aeromexico-logo.svg">
        <h1 class="rsCustomer-story-title">Triple Your Online Sales by Modernizing Your Stack</h1>
        <p class="rsCustomer-story-p">
          See how Aeromexico revamped their interaction with customers using modern applications, platforms and devices.
        </p>
        <a class="rsCustomer-story-cta" href="#">Read Their Story</a>
      </div>
    </div>
  </div>
</div>
